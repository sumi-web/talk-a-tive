{
  "name": "server2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:generate && cross-env NODE_ENV=development ts-node-dev --transpile-only -r tsconfig-paths/register --respawn --exit-child src/index.ts",
    "prod": "npm run build && npm run start",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "clean:build": "rm -rf dist",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "check:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@swc/core": "^1.3.31",
    "@swc/helpers": "^0.4.14",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "chalk": "4.1.2",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "axios": "^1.4.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "nocache": "^4.0.0",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  }
}
